@model IEnumerable<Test.Models.UserContent>
@{
    ViewBag.Title = "Index";
}
<h2>User List</h2>
<table class="table">
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Three Digit Number</th>
        <th>Wins</th>
        <th>Losses</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Username)</td>
            <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
            <td>@Html.DisplayFor(modelItem => item.LastName)</td>
            <td>@Html.DisplayFor(modelItem => item.ThreeDigitNumber)</td>
            <td>@Html.DisplayFor(modelItem => item.Wins)</td>
            <td>@Html.DisplayFor(modelItem => item.Losses)</td>
            <td>
               <a href="/Test/Edit/@item.Username">Edit</a>
               <a href="/Test/Create">Create</a>
               <a href="/Test/Delete/@item.Username">Delete</a>
            </td>
        </tr>
    }
</table>


